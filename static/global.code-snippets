            "                createdBefore: positiveIntParser,",
            "                createdAfter: positiveIntParser,",
            "            },",
            "        }),",
            "        {",
            "            get({ createdAfter, createdBefore, ...rest }) {",
            "                return {",
            "                    createdAt: [isNonNullable(createdAfter) ? dayjs(createdAfter) : undefined, isNonNullable(createdBefore) ? dayjs(createdBefore) : undefined],",
            "                    ...rest,",
            "                }",
            "            },",
            "            set({ createdAt, ...rest }) {",
            "                return {",
            "                    createdAfter: createdAt?.[0]?.valueOf(),",
            "                    createdBefore: createdAt?.[1]?.valueOf(),",
            "                    ...rest,",
            "                }",
            "            },",
            "            dependOnGet: false,",
