            "    if (obj === undefined || obj === null) return \"unknown\"",
            "    return obj.constructor.name",
            "}",
            "",
            "function getStringProperty(obj: unknown, key: string): string | undefined {",
            "    return ((obj ?? {}) as any)[key]",
