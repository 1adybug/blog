    "Editor": {
        "scope": "javascript,javascriptreact,typescript,typescriptreact",
        "prefix": "editor",
        "body": [
            "import { ComponentProps, FC, useEffect } from \"react\"",
            "",
            "import { Button, Form, Input, Modal } from \"antd\"",
