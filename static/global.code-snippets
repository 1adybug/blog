            "",
            "    const isRequesting = isLoading || isPending",
            "",
            "    useEffect(() => {",
            "        if (!open || !data) return",
            "        form.setFieldsValue(data)",
            "        return () => form.resetFields()",
            "    }, [open, data, form])",
            "",
            "    function onFinish(value: Add${1}Params) {",
            "        if (isUpdate) update${1}({ ...value, id: id! })",
            "        else add${1}(value)",
            "    }",
